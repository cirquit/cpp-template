

 set(PACKAGE_VERSION @cpp_VERSION@)
 if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL "@cpp_VERSION_MAJOR@")
     if ("${PACKAGE_FIND_VERSION_MINOR}" EQUAL "@cpp_VERSION_MINOR@")
         set(PACKAGE_VERSION_EXACT TRUE)
     elseif("${PACKAGE_FIND_VERSION_MINOR}" LESS "@cpp_VERSION_MINOR@")
         set(PACKAGE_VERSION_COMPATIBLE TRUE)
     else()
         set(PACKAGE_VERSION_UNSUITABLE TRUE)
     endif()
 else()
     set(PACKAGE_VERSION_UNSUITABLE TRUE)
 endif()